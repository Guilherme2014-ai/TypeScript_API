{
  "name": "api_typescript",
  "version": "1.0.0",
  "description": "A Node Application made with Type-Script",
  "main": "./dist/server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "nodemon ./src/server.ts",
    "build:sucrase": "sucrase ./src -d ./dist --transforms typescript,imports",
    "build:tsc": "tsc --build",
    "build:tsc:clean": "tsc --build --clean",
    "migration:create": "npx typeorm migration:create -n",
    "migration:run": "sucrase-node ./node_modules/typeorm/cli.js migration:run",
    "migration:run:undo": "sucrase-node ./node_modules/typeorm/cli.js migration:revert",
    "entity:create": "npx typeorm entity:create -n"
  },
  "keywords": [],
  "author": "Guilherme",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.3",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "sucrase": "^3.20.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
